    "type": "git",
    "url": "https://github.com/PAYONE-GmbH/PCP-server-nodeJS-SDK.git"
  },
  "devDependencies": {
    "@eslint/js": "9.8.0",
    "@types/node": "22.1.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "9.8.0",
    "globals": "15.9.0",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "typescript-eslint": "8.0.1",
    "vitest": "2.0.5"
  },
  "dependencies": {
    "node-fetch": "3.3.2"
